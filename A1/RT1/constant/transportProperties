FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "constant";
    object      transportProperties;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

phases (water air);

water
{
    transportModel  Newtonian;
    
    nu              1e-03;
    rho             3;
}

air
{
    transportModel  Newtonian;
    
    nu              3e-03;
    rho             1;
}


sigma 				0.000333;

